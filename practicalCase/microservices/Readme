#installation d'express et des types: 
cd 'src\students'
npm init -y
npm install express
npm install --save-dev typescript ts-node @types/express
npm install cors
npm install --save-dev typescript ts-node @types/cors

#Lancement de l'api students
npx ts-node src/api/app.ts


##Partie front
#installation du serveur web 
# depuis la racine du projet :
cd src\web
npm init -y
npm install -g live-server

#Lancement du serveur web
npx live-server
